{
  "compilerOptions": {
    "baseUrl": ".",
    "jsx": "preserve",
    "strict": true,
    "target": "ES2015",
    // "suppressImplicitAnyIndexErrors": true,
    "module": "esnext",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "lib": ["esnext", "dom"],
    "resolveJsonModule": true,
    "types": ["node", "vite/client"],
    "paths": {
        "@site/*": ["./site/*"],
    },
  }
}
